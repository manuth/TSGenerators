{
  "name": "@manuth/generator-ts-project",
  "version": "3.0.6",
  "description": "A Set of Generators for Creating Projects Written in TypeScript",
  "author": "Manuel Thalmann <m@nuth.ch>",
  "license": "MIT",
  "keywords": [
    "yo",
    "yoeoman-generator",
    "Generator",
    "TypeScript"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "homepage": "https://github.com/manuth/TSProjectGenerator/tree/main/packages/generator-ts-project#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/manuth/TSProjectGenerator.git",
    "directory": "packages/generator-ts-project"
  },
  "bugs": {
    "url": "https://github.com/manuth/TSProjectGenerator/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run copy-files && npm run compile",
    "copy-files": "cd ../.. && npm run copy-files",
    "compile": "tsc -b tsconfig.build.json",
    "watch-compile": "npm run compile -- --watch",
    "rebuild": "npm run clean && npm run build",
    "watch": "concurrently --raw \"npm run copy-files -- -- --watch | shusher\" \"npm run watch-compile\"",
    "clean": "npm run compile -- --clean && rimraf ./lib",
    "lint-code-base": "eslint --max-warnings 0 --ignore-pattern \"!.eslintrc.js\"",
    "lint-code": "npm run lint-code-base -- --ext .js,.jsx,.ts,.tsx ./src .eslintrc.js",
    "lint-code-ide": "npm run lint-code || exit 0",
    "lint-docs": "markdownlint ./templates/**/*.md  ./templates/**/*.md.ejs",
    "lint": "npm run lint-code && npm run lint-docs",
    "lint-ide": "npm run lint-code-ide && npm run lint-docs || exit 0",
    "test": "mocha && tsd",
    "prepare": "npm run patchTypeScript && npm run rebuild",
    "patchTypeScript": "ts-patch install && ts-patch --persist"
  },
  "dependencies": {
    "@manuth/eslint-plugin-typescript": "^3.2.5",
    "@manuth/extended-yo-generator": "^11.0.4",
    "@manuth/package-json-editor": "^2.2.3",
    "@manuth/temp-files": "^2.0.7",
    "@types/mute-stream": "^0.0.1",
    "@types/vscode": "^1.59.0",
    "chalk": "^4.1.2",
    "comment-json": "^4.1.1",
    "dedent": "^0.7.0",
    "diff": "^5.0.0",
    "eol": "^0.9.1",
    "fs-extra": "^10.0.0",
    "inquirer": "^8.1.2",
    "lodash.camelcase": "^4.3.0",
    "lodash.kebabcase": "^4.1.1",
    "mute-stream": "^0.0.8",
    "npm-which": "^3.0.1",
    "parse-pkg-name": "^2.0.0",
    "ts-morph": "^12.0.0",
    "types-eslintrc": "^1.0.3",
    "types-tsconfig": "^1.2.1",
    "upath": "^2.0.1",
    "validate-npm-package-name": "^3.0.0",
    "yaml": "^1.10.2",
    "yosay": "^2.0.2"
  },
  "devDependencies": {
    "@manuth/extended-yo-generator-test": "^11.0.4",
    "@manuth/generator-ts-project-test": "^3.0.6",
    "@manuth/tsconfig": "^2.0.3",
    "@manuth/typescript-eslint-plugin": "^1.4.2",
    "@types/dedent": "^0.7.0",
    "@types/diff": "^5.0.1",
    "@types/eslint": "^7.28.0",
    "@types/fs-extra": "^9.0.12",
    "@types/inquirer": "^7.3.3",
    "@types/lodash.camelcase": "^4.3.6",
    "@types/lodash.kebabcase": "^4.1.6",
    "@types/mocha": "^9.0.0",
    "@types/mock-require": "^2.0.0",
    "@types/node": "^16.7.5",
    "@types/npm-packlist": "^1.1.2",
    "@types/npm-which": "^3.0.1",
    "@types/sinon": "^10.0.2",
    "@types/ts-nameof": "^4.2.1",
    "@types/validate-npm-package-name": "^3.0.3",
    "@types/yosay": "2.0.0",
    "concurrently": "^6.2.1",
    "detect-newline": "^3.1.0",
    "eslint": "^7.32.0",
    "markdownlint-cli": "^0.28.1",
    "mocha": "^9.1.1",
    "mock-require": "^3.0.3",
    "mock-stdin": "^1.0.0",
    "npm-packlist": "^3.0.0",
    "randexp": "^0.5.3",
    "rimraf": "^3.0.2",
    "shusher": "^0.1.1",
    "sinon": "^11.1.2",
    "source-map-support": "^0.5.19",
    "ts-nameof": "^5.0.0",
    "ts-patch": "^1.4.4",
    "tsd": "^0.17.0",
    "tslint": "^6.1.3",
    "typescript": "^4.4.2",
    "util.chdir": "0.0.43",
    "yeoman-test": "^6.2.0",
    "yo": "^4.3.0"
  },
  "tsd": {
    "directory": "./type-tests",
    "compilerOptions": {
      "strict": false
    }
  }
}
